<!DOCTYPE html>
<html>
  <head>
    <title>Recipes Galore</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Recipes Galore</h1>
    <p>There Shall be a list!</p>
    <% recipes.forEach(function(recipe){ %>
        <p>
            <h1><%= recipe.name %></h1>
            <h3><%= recipe.image %></h3>
            <h4><%= recipe.description %></h4>
            <h5><%= recipe.ingredients[0] %></h5>
            <h5><%= recipe.instructions %></h5>
        </p>
    <%})%>
  </body>
</html>